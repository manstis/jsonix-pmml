{
  "name": "jsonix-pmml",
  "version": "1.0.0",
  "description": "Unmarshal PMML XML to JS",
  "main": "main.ts",
  "repository": "https://github.com/manstis/jsonix-pmml.git",
  "author": "Michael Anstis <manstis@redhat.com>",
  "license": "Apache-2.0",
  "files": [
    "./src/*.ts",
    "./src/*.js"
  ],
  "scripts": {
    "generate": "java -jar node_modules/jsonix/lib/jsonix-schema-compiler-full.jar -d ./src/shared -b ./src/resources/bindings.xjb ./src/resources/pmml-4-4.xsd ",
    "build": "npx tsc",
    "helloworld": "npx ./dist/main.js"
  },
  "devDependencies": {
    "jsonix": "^3.0.0"
  }
}
